{
  "name": "simple-node-swagger-showcase",
  "version": "1.0.0",
  "description": "Simple Node Swagger Showcase",
  "private": true,
  "dependencies": {
    "awesome-phonenumber": "2.46.0",
    "accesscontrol": "2.2.1",
    "axios": "0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.15.2",
    "dotenv": "2.0.0",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "http-status-codes": "1.4.0",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.27.0",
    "pg": "8.2.2",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.1.4",
    "tslog": "3.1.1",
    "tsoa": "3.5.2",
    "typeorm": "0.2.25",
    "underscore": "1.12.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/helmet": "4.0.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "13.13.5",
    "@types/swagger-ui-express": "4.1.2",
    "@types/underscore": "1.10.24",
    "concurrently": "5.3.0",
    "nodemon": "2.0.7",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typeorm": "0.2.25",
    "typescript": "4.1.5"
  },
  "main": "dist/StartServer.js",
  "scripts": {
    "prebuild": "npm run swagger",
    "swagger": "tsoa spec",
    "build": "./node_modules/rimraf/bin.js ./dist && ./node_modules/typescript/bin/tsc -p .",
    "dev": "concurrently \"NODE_ENV=development nodemon\" \"nodemon -x tsoa spec\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/StartServer.ts"
  },
  "engines": {
    "node": "12.18.x"
  },
  "author": "Ariaroo Efe"
}
